class Solution {
public:
    bool divisorGame(int n) {
        if(n == 1)
            return 0;
        if(n == 2)
            return 1;
        vector<int> dp(n+1);
        dp[0] = 0;
        dp[1] = 0;
        dp[2] = 1;
        for(int i = 3; i<=n; i++)
            dp[i] = dp[i-2];
        return dp[n];

    }
};

Time Complexity: O(N)
Space Complexity: O(N)
